package com.skillstorm.week1.day4;

public class Vehicle {

	private String make;
	private String model;
	private int year;
	private String color;
	
	public Vehicle() {
		
	}
	
	public Vehicle(String make, String model, int year, String color) {
		super();
		this.make = make;
		this.model = model;
		this.year = year;
		this.color = color;
	}
	
	
}
