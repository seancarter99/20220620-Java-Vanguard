package com.skillstorm.conf;

public class ChinookDbConnection {

	static {
		try {
			Class.forName("com.mysql.cj.jdbc.Driver");
		} catch (ClassNotFoundException e) {
			e.printStackTrace();
		}
	}
	
	private static ChinookDbConnection connection = new ChinookDbConnection();
	
	private ChinookDbConnection() {
		
	}
	
	public static ChinookDbConnection getInstance() {
		return connection;
	}
	
	
}
